[target.aarch64-linux-android]
# ADJUST THIS PATH TO YOUR NDK INSTALLATION
linker = "<ANDROID_NDK_HOME>/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android28-clang"

[env]
# ADJUST THESE PATHS TO YOUR NDK INSTALLATION
CC_aarch64_linux_android = "<ANDROID_NDK_HOME>/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android28-clang"
CXX_aarch64_linux_android = "<ANDROID_NDK_HOME>/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android28-clang++"
AR_aarch64_linux_android = "<ANDROID_NDK_HOME>/toolchains/llvm/prebuilt/linux-x86_64/bin/llvm-ar"
BINDGEN_EXTRA_CLANG_ARGS_aarch64_linux_android = "--sysroot=<ANDROID_NDK_HOME>/toolchains/llvm/prebuilt/linux-x86_64/sysroot"

# ADJUST THESE PATHS TO YOUR ONNX RUNTIME INSTALLATION
ORT_LIB_LOCATION = "<PATH_TO_ONNX_RUNTIME>/jni/arm64-v8a"
ORT_INCLUDE_DIR = "<PATH_TO_ONNX_RUNTIME>/headers"

ANDROID_NDK_HOME = "<ANDROID_NDK_HOME>"
ANDROID_NDK = "<ANDROID_NDK_HOME>"
